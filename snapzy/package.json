{
  "name": "snapzy",
  "private": true,
  "version": "1.0.0",
  "packageManager": "pnpm@9.6.0",
  "workspaces": [
    "services/*",
    "packages/*"
  ],
  "scripts": {
    "build": "pnpm -r build",
    "dev": "pnpm -r --parallel dev",
